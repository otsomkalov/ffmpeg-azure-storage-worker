{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Storage": {
    "ConnectionString": "<Azure Storage account connection string>",
    "Input": {
      "Container": "input",
      "Queue": "input"
    },
    "Output": {
      "Container": "output",
      "Queue": "output"
    }
  },
  "FFMpeg": {
    "Path": "<FFMpeg full path>",
    "Arguments": "<Arguments for FFMpeg conversion>",
    "TargetExtension": "<Target extension for the converted file w/o dot>"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "<AppInsights key>"
  },
  // Delay of background job execution, in seconds
  "Delay": "<Delay between job executions in hh:mm:ss format>"
}
